<!DOCTYPE html>

<title>Package</title>

{% block body %}
    <h1>Package</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ package.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ package.name }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ package.comment }}</td>
            </tr>
            <tr>
                <th>Weight</th>
                <td>{{ package.weight }}</td>
            </tr>
            <tr>
                <th>DeliveryCost</th>
                <td>{{ package.deliveryCost }}</td>
            </tr>
            <tr>
                <th>Tracking</th>
                <td>{{ package.tracking }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ package.status }}</td>
            </tr>
            <tr>
                <th>CreateDate</th>
                <td>{{ package.createDate ? package.createDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdateDate</th>
                <td>{{ package.updateDate ? package.updateDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('package_index') }}">back to list</a>

    <a href="{{ path('package_edit', {'id': package.id}) }}">edit</a>

    {{ include('package/_delete_form.html.twig') }}
{% endblock %}
