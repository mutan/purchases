            </ul>
            <div class="sidenav-bg mask-strong"></div>
        </div>
        <!--/. Sidebar navigation -->

        <!-- Navbar -->
        <nav class="navbar fixed-top navbar-expand-lg navbar-dark mdb-color darken-3 scrolling-navbar">
            <div class="container">

                <!-- SideNav slide-out button -->
                <div class="float-left">
                    <a href="" data-activates="slide-out" class="button-collapse black-text"><i class="fas fa-bars"></i></a>
                </div>

                <a class="navbar-brand" href="#" target="_blank">
                    <strong>Совместные покупки<span class="d-none d-md-inline"> в интернет-магазинах США</span></strong>
                </a>

                <!-- Collapse -->
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <!-- Links -->
                <div class="collapse navbar-collapse" id="navbarSupportedContent">

                    <!-- Left -->
                    <ul class="navbar-nav mr-auto">
                        <li class="nav-item active">
                            <a class="nav-link" href="#">Home
                                <span class="sr-only">(current)</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#" target="_blank">About MDB</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#" target="_blank">Free download</a>
                        </li>
                    </ul>

                    <!-- Right -->
                    <ul class="navbar-nav nav-flex-icons">
                        <li class="nav-item">
                            {% if is_granted('ROLE_USER') %}
                                <a class="nav-link" href="{{ path('app_logout') }}">
                                    <i class='fa fa-user'></i> {{ app.user.name }}
                                </a>
                            {% else %}
                                <a class="nav-link" href="{{ path('app_login') }}">
                                    <i class='fa fa-sign-in-alt'></i> Войти
                                </a>
                            {% endif %}
                        </li>

            </ul>
            <!--/Navbar links-->
        </nav>
        <!-- /.Navbar -->

    </header>
    <!--Main Navigation-->

        <main class="mt-5 pt-2">
            <!-- User switch -->
            {% if is_granted('ROLE_PREVIOUS_ADMIN') %}
                <div class="alert alert-danger text-center mb-0"
                     style="border-radius: 0; font-size: 0.9rem; padding: .3rem 1.25rem;"
                >
                    You are currently switched to user <strong>{{ app.user.name }} ({{ app.user.email }})</strong>.
                    <a class="alert-link" href="{{ path('app_homepage', {'_switch_user' : '_exit'}) }}"> <i class="fas fa-sign-out-alt ml-2"></i> Exit</a>
                </div>
            {% endif %}
            <!-- /User switch -->

            <!-- Flash messages -->
            {% for label, messages in app.flashes %}
                {% for message in messages %}
                    <div class="alert alert-{{ label }} alert-dismissible fade show" role="alert">
                        {{ message }}
                        <button type="button" class="close" data-dismiss="alert">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                {% endfor %}
            {% endfor %}
            <!-- /Flash messages -->

            <div class="container mt-5">
                {% block body %}{% endblock %}
            </div>
        </main>

        <!-- Footer -->
        <footer class="page-footer font-small unique-color-dark">

            <div class="info-color-dark">
                <div class="container">

                    <div class="row py-3 d-flex align-items-center">

                        <div class="col-md-4 text-center mb-4 mb-md-0">
                            <h6 class="mb-0">Get connected with us on social networks!</h6>
                        </div>

                        <div class="col-md-4 text-center mb-4 mb-md-0">
